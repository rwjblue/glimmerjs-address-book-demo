<div class="container-fluid">
  <h1>Address Book</h1>

  <div class="row">
    <div class="col">
      <ul class="list-group">
        {{#each contacts key="@index" as |contact|}}
          <li class="list-group-item">
            {{contact.fullName}}
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
